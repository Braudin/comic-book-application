{
    "parser": "babel-eslint",
    "globals": {
        "window": true,
        "ActiveXObject": true,
        "Enumerator": true
    },
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true
    },
    "extends": [
        "standard",
        "prettier",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off",
        "no-use-before-define": "off"
    }
}